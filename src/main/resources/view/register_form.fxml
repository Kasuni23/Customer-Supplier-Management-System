<AnchorPane fx:id="root" prefHeight="538.0" prefWidth="379.0" style="-fx-background-color: #ff9ff3;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.customersuppliermanagementsystem.controller.RegisterFormController">
